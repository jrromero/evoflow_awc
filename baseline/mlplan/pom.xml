  		<artifactId>mlplan-sklearn</artifactId>
  		<version>0.2.7</version>
  	</dependency>
	<dependency>
  		<groupId>ai.libs</groupId>
  		<artifactId>mlplan-weka</artifactId>
  		<version>0.2.7</version>
  	</dependency>
	<dependency>
	  <groupId>ai.libs</groupId>
	  <artifactId>hasco-core</artifactId>
	  <version>0.2.7</version>
	</dependency>
  </dependencies>
</project>